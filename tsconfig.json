{
  "compilerOptions": {
    "target": "es2022",
    "module": "es2022",
    "lib": ["dom", "dom.iterable", "es2022"],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "isolatedModules": true,
    "outDir": "./js"
  },
  "include": ["src/ts"]
}
